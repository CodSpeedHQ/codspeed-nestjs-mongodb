{
  "name": "codspeed-nestjs-mongodb",
  "version": "1.0.0",
  "description": "CodSpeed NestJS MongoDB example",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "bench:tinybench": "NODE_ENV=test ts-node --swc -r tsconfig-paths/register src/bench.ts",
    "bench:tinybench:testcontainers": "NODE_ENV=test ts-node --swc -r tsconfig-paths/register src/bench-testcontainers.ts",
    "bench:vitest": "vitest bench",
    "bench:vitest:e2e": "vitest -c vitest.config.e2e.mts bench",
    "bench:vitest:e2e:testcontainers": "TESTCONTAINERS=true vitest -c vitest.config.e2e.mts bench",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run -c ./vitest.config.e2e.ts"
  },
  "dependencies": {
    "@nestjs/common": "10.0.3",
    "@nestjs/core": "10.0.3",
    "@nestjs/mongoose": "10.0.0",
    "@nestjs/platform-express": "10.0.3",
    "mongoose": "7.3.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@codspeed/tinybench-plugin": "^3.1.0",
    "@codspeed/vitest-plugin": "^3.1.0",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@swc/core": "^1.3.72",
    "@swc/cli": "^0.3.0",
    "@swc/helpers": "^0.5.1",
    "@testcontainers/mongodb": "^10.6.0",
    "@types/express": "4.17.17",
    "@types/node": "20.3.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "prettier": "2.8.8",
    "regenerator-runtime": "^0.13.11",
    "supertest": "6.3.3",
    "tinybench": "^2.5.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": "18.15.0"
  },
  "packageManager": "pnpm@8.6.10"
}
